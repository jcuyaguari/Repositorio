	<queryString>
		<![CDATA[SELECT
formulacion.formId,
formulacion.formProducto,
producto.prodNombre,
formulacion.formFecha,
detalleformulacion.dfInsumo,
detalleformulacion.dfInsumonombre,
detalleformulacion.dfPorcentaje

FROM
producto
INNER JOIN formulacion ON formulacion.formProducto = producto.proIdd
